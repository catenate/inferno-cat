# |sort|uniq

acme:
	Edit X/$$1/D
	Edit X/dis/D
	Edit X/log/D
	Load

aide:
	/lib/aide
	du -n | grep '[Aa]ide'
	grep -n ':$' Aide /dev/null | sed 's,: , ,g;s,:$, ,g' | fmt

dump:
	Dump /github/$$1/dump

font:
	/github/acme-fonts/test/1/acme/Font
	/usr/jason/lib/wmsetup

