# |sort|uniq

aide:
	/github/inferno-cat/lib/aide
	du -n /github | grep '/Aide$'
	ls /github/*/Aide

branch:
	nsh git branch -a

change:
	git
	git add --all .
	git diff
	git diff --staged
	git | grep -v '^#' | cicd field 2
	nsh git diff
	nsh git diff --staged

clone:
	.git/info/exclude

commit:
	git log --decorate --oneline --graph -n 5
	git log --decorate --oneline --graph --all -n 5
	git show HEAD
	log github git commit -a -m ''
	log github git commit -a -m 'Update Aide.'

config:
	git config --get remote.origin.url
	git config --global user.email 'jason.catena@seedcx.com'
	git config --global user.name 'Jason Catena'

github:
	web 'github.com/$$1/$$2'

push:
	git fetch --all
	git rebase c/master
	log github git push c master

rebase:
	git rebase --abort
	git rebase --continue
	git rebase --skip

reflog:
	git reflog | sed 5q
	log $$1 git cherry-pick 0b0b7e7
	log $$1 git reset --hard 'HEAD@{3}'
	log $$1 git reset --hard HEAD
	log $$1 git reset --hard $$2/$$3

remote:
	git remote
	git remote add c catenate:catenate/$$1.git
	git remote get-url c
	git remote remove c
	git remote show c
