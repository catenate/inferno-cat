|aide edit sort
|aide git change
change:
	git
	git add --all .
	git diff
	git diff --staged
	git | grep -v '^#' | cicd field 2
	nsh git diff
	nsh git diff --staged

|aide git commit
|aide inferno acme inferno-cat
|aide inferno aide
	
commit:
	log github git commit -a -m 'Add a library of blocks of commands with parameter substitution.'

dump:
	Dump /github/inferno-cat/dump
	Load /github/inferno-cat/dump
	
edit:
	|sort|uniq
	gedit scratch

history:
	git log --decorate --graph --oneline --all | sed 10q
	git show 85ad035
	log
	log git

remote:
	git remote
	git remote add catenate catenate:catenate/inferno-cat.git
	git remote get-url catenate
	git remote remove catenate
	git remote show catenate
	log github git push catenate master

search:
	Mark catenate custom inferno
	du -n | grep -v '\.git' | grep Aide
	du -n | grep -v '\.git' | grep sh-inferno
	du | grep -v '\.git'
	ls -F | mc

wc:
	cat sh-inferno/* | grep -v '^[	 }{]*$' | grep -v '^[	 ]*#' |  wc -l

web:
	web 'http://github.com/catenate/inferno-cat'
