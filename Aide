change:
	git
	git add --all .
	git diff
	git diff --staged
	nsh git diff --staged
	
commit:
	log commit git commit -a -m 'Revive log to capture host commands.'

dump:
	Dump /github/inferno-cat/dump
	Load /github/inferno-cat/dump
	
edit:
	|sort|uniq
	gedit scratch

history:
	git log --decorate --graph --oneline --all | sed 10q
	git show 85ad035
	log
	log git

origin:
	git push origin master
	host key catenate

search:
	Mark catenate custom inferno
	du -n | grep -v '\.git' | grep Aide
	du -n | grep -v '\.git' | grep sh-inferno
	du | grep -v '\.git'
	ls -F | mc

wc:
	cat sh-inferno/* | grep -v '^[	 }{]*$' | grep -v '^[	 ]*#' |  wc -l

web:
	web 'http://github.com/catenate/inferno-cat'
